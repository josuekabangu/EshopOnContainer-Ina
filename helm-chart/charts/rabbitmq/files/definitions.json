{
  "vhosts": [
    { "name": "/" }
  ],
  "users": [
    {
      "name": "admin",
      "password": "admin",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha-all",
      "pattern": ".*",
      "definition": {
        "ha-mode": "all"
      },
      "priority": 0,
      "apply-to": "all"
    }
  ]
}
